<% if @current_user.nil? %>
  <%= link_to new_user_path do %>
    Sign Up
  <% end %>
  Login
<% else %>
  <%= flash[:notice] %>
  <%= link_to new_user_path do %>
    Logout
  <% end %>
<% end %>



<% @posts.each do |post| %>
  <p><%= post.message %></p>
<% end %>

<%= link_to new_post_path do %>
  New post
<% end %>

